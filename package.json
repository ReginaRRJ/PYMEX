        "@fontsource/poppins": "^5.2.5",
        "@sap/hana-client": "^2.24.24",
        "@vitejs/plugin-legacy": "^6.0.2",
        "autoprefixer": "^10.4.21",
        "axios": "^1.8.4",
        "base": "^3.0.0",
        "bcryptjs": "^3.0.2",
        "bl": "^6.1.0",
        "components": "^0.1.0",
        "cors": "^2.8.5",
        "cypress": "^14.3.0",
        "dbapi": "^0.2.1",
        "dotenv": "^16.4.7",
        "express": "^4.21.2",
        "framer-motion": "^12.12.1",
        "jsonwebtoken": "^9.0.2",
        "morgan": "^1.10.0",
        "mssql": "^11.0.1",
        "pandas": "^0.0.3",
        "pg": "^8.13.3",
        "process": "^0.11.10",
        "react": "^19.1.0",
        "react-dom": "^19.1.0",
        "react-router-dom": "^7.4.1",
        "react-select": "^5.10.1",
        "readable-stream": "^4.7.0",
        "sap": "^0.0.2",
        "sklearn": "^1.0.0",
        "stream-browserify": "^3.0.0",
        "swagger-jsdoc": "^6.2.8",
