  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "jest --coverage"
  },
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "./coverage",
    "coverageReporters": [
      "lcov",
      "text"
    ],
    "testMatch": [
      "**/?(*.)+(test).[jt]s?(x)"
    ],
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest"
    },
    "testEnvironment": "jsdom",
    "setupFiles": [
      "<rootDir>/jest.setup.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|webp|svg|css|less|scss|sass)$": "<rootDir>/__mocks__/fileMock.js"
    }
  },
  "dependencies": {
