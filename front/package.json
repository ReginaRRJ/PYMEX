    "@mui/material": "^7.1.0",
    "bl": "^6.1.0",
    "browser": "^0.2.6",
    "concurrently": "^9.1.2",
    "framer-motion": "^12.12.1",
    "process": "^0.11.10",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "^7.4.1",
    "react-select": "^5.10.1",
